import "funcs.lg";


func int main() /* Function def */ {
    rational <int, int> c = {1, 2}; /* Declaration */ 
    curve y (x, a, b) = a * sin($x) + b * cos($x) ^ 2 + a * b; /* Declaration */ 
    curve w (x) = $x^4 + 3*$x^2 - 2*$x; /* Declaration */ 

    curve z(y) = input_poly(5) / w($y); /* Declaration */   

    curve z2(y) = z * $y; /* Declaration */  

    curve y1 = D[y, x]; /* Declaration */ 

    z2 = D[z2, x]; /* Assignment */ 
    curve z3 = D[y*z2, x]; /* Declaration */ 

    print_curve(z3); /* Function Call */ 

    return 0; /* Return */ 
}
